#Keyset file for Lawyer Race
[player1]
down=down 
up=up    
right=right
left=left
stop=rctrl
[player2]
down=s
up=w
right=d
left=a
stop=lctrl
[player3]
down=k
up=i
right=l
left=j
stop=space
[player4]
down=num_5
up=num_8
right=num_6
left=num_4
stop=num_0
[player1.1]   #for 1 player game only
stop=space

# Available keys:
# a-z
# 0-9
# up
# down
# left
# right 
# num_0 - num_9
# rctrl 
# lctrl 
# rshift
# lshift
# lalt
# altgr
# tab 
# insert
# delete
# home
# end 
# pgup
# pgdown
# backspace
# return
# space

